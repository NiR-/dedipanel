{% extends "::layout.html.twig" %}

{% import 'SyliusResourceBundle:Macros:actions.html.twig' as sylius_actions %}
{% import '::buttons.html.twig' as dedipanel_buttons %}

{% block title %}
    {{ dedipanel_buttons.back(path('dedipanel_steam_index'), 'steam.title.steam'|trans) }}
{% endblock %}
        
{% block content %}
    <h2>{{ 'steam.title.add'|trans }}</h2>

    <form action="{{ path('dedipanel_steam_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        {{ sylius_actions.create() }}
    </form>
{% endblock %}