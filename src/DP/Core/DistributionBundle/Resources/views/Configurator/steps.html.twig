<div class="symfony-block-steps">
    {% for i in 1..count %}

      {% if i == index + 1 %}
          <span class="selected">{{ 'configurator.step'|trans }} {{ i }}</span>
      {% else %}
          <span>{{ 'configurator.step'|trans }} {{ i }}</span>
      {% endif %}

      {% if i != count %}
        &gt;
      {% endif %}
    {% endfor %}
</div>
