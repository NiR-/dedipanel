{
    "^utilisateur/": {
        "routes": {
            "utilisateur/login$": {"route": "utilisateurs/utilisateur/login"}, 
            "utilisateur/logout$": {"route": "utilisateurs/utilisateur/logout"}, 
            "utilisateur/menu$": {"route": "utilisateurs/utilisateur/menu"}, 
            "utilisateur/profil$": {"route": "utilisateurs/utilisateur/profil"}
        }
    }, 
    
    "^utilisateurs": {
        "route": "utilisateurs/utilisateurs/show", 
        
        "routes": {
            "utilisateurs/groups": {
                "route": "utilisateurs/groups/show", 
                "routes": {
                    "utilisateurs/groups/add": {"route": "utilisateurs/groups/add"}
                }
            }, 
            
            "utilisateurs/add$": {"route": "utilisateurs/utilisateurs/add"}, 
            "utilisateurs/del/$$uid$": {
                "route": "utilisateurs/utilisateurs/del", "vars": {"uid": "[0-9]+"}
            }
        }
    }, 
    
    "^groups": {
        "route": "utilisateurs/groups/show", 
        "routes": {
            "groups/add": {"route": "utilisateurs/groups/add"}
        }
    } 
}