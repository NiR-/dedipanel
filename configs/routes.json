{
    "^utilisateurs": {
        "route": "utilisateurs/utilisateurs/show",
        "routes": {
            "utilisateurs/groups": {
                "route": "utilisateurs/groups/show", 
                "routes": {
                    "utilisateurs/groups/add": {"action": "add"}
                }
            }, 
            "utilisateurs/add$": {"action": "add"}, 
            "utilisateurs/edit/$$uid$": {"action": "edit", "vars": {"uid": "[0-9]+"}}, 
            "utilisateurs/del/$$uid$": {
                "action": "del", "vars": {"uid": "[0-9]+"}
            } 
        }
    }, 
    
    "^utilisateur": {
        "route": "utilisateurs/utilisateur/login", 
        "routes": {
            "utilisateur/login$": {}, 
            "utilisateur/logout$": {"action": "logout"}, 
            "utilisateur/menu$": {"action": "menu"}, 
            "utilisateur/profil$": {"action": "profil"}
        }
    }, 
    
    "^groups": {
        "route": "utilisateurs/groups/show", 
        "routes": {
            "groups/add": {"action": "add"}, 
            "groups/edit/$$gid$": {"action": "edit", "vars": {"gid": "[0-9]+"}}, 
            "groups/del/$$gid$": {"action": "del", "vars": {"gid": "[0-9]+"}}
        }
    },
    
    "^vm": {
        "route": "vm/vm/show", 
        
        "routes": {
            "vm/add$": {"action": "add"},
            "vm/edit/$$id$": {"action": "edit", "vars": {"id": "[0-9]+"}},
            "vm/del/$$id$": {"action": "del", "vars": {"id": "[0-9]+"}},
            "vm/test/$$id$": {"action": "testConnexion", "vars": {"id": "[0-9]+"}
            }
        }
    }, 

    "^steam": {
        "route": "steam/steam/show", 

        "routes": {
            "steam/add": {"action": "add"},
            "steam/del/$$mode/$$id$": {"action": "del", "vars": {"mode": "soft|hard", "id": "[0-9]+"}},
            "steam/edit/$$id$": {"action": "edit", "vars": {"id": "[0-9]+"}},
            "steam/regen/$$id$": {"action": "regenConfig", "vars": {"id": "[0-9]+"}}, 
            "steam/$$state/$$id$": {
                "action": "state",
                "vars": {
                    "state": "start|stop|restart",
                    "id": "[0-9]+"
                }
            },
            "steam/plugins/$$plugin/$$id$": {
                "route": "steam/plugins/install", 
                "vars": {"plugin": "mm|amx|amxx|mmsrc|srcm|es", "id": "[0-9]+"}
            },
            
            "steam/cfg/$$id": {
                "route": "steam/cfg/show", 
                "vars": {"id": "[0-9]+"},
                
                "routes": {
                    "steam/cfg/$$id/$$dir/?$": {"vars": {"id": "[0-9]+", "dir": ".+"}}
                }
            },
            "steam/cfg/add/$$id/$$dir/?$": {
                "route": "steam/cfg/add",
                "vars": {"id": "[0-9]+", "dir": ".*"}
            },
            "steam/cfg/edit/$$id/$$file/?$": {
                "route": "steam/cfg/edit",
                "vars": {"id": "[0-9]+", "file": ".+"}
            },
            "steam/cfg/del/$$id/$$file/?$": {
                "route": "steam/cfg/del",
                "vars": {"id": "[0-9]+", "file": ".+"}
            }
        }
    } 
}